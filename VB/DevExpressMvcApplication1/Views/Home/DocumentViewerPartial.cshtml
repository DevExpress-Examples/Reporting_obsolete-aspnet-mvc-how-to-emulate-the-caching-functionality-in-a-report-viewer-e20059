@model DevExpressMvcApplication1.Controllers.HomeController.TestModel

@Html.DevExpress().DocumentViewer(settings =>{
    // The following settings are necessary for a Report Viewer.
    settings.Name = "DocumentViewer1";
    settings.Report = Model.Report;
    // Callback and export route values specify the corresponding controllers and their actions.
    // These settings are necessary as well.
    settings.CallbackRouteValues = new { Controller="Home", Action="DocumentViewerPartial" };
    settings.ExportRouteValues = new { Controller="Home", Action="ExportDocumentViewerPartial" };
    }).GetHtml()
    